<%- include('../templates/header'); -%>

<div class="container">
	<h1 class="mt-4 text-center">Final Results</h1>
	<p class="text-center mb-5">ðŸš€ðŸš€ðŸš€</p>

	<div class="d-flex flex-row flex-wrap justify-content-around">
		<div class="image-container mb-5">
			<h2 class="text-center"><%= contestants.female[0].name[0] %></h2>
			<img
				src="../assets/<%= contestants.female[0].name[1] %>.jpeg"
				class="img-fluid"
			/>
			<h3 class="text-center mb-3">
				Total Votes : <%= contestants.female[0].Vote %>
			</h3>
			<div class="d-flex flex-row justify-content-between">
				<h6>
					<%= contestants.female[1].name[0] %> : <%= contestants.female[1].Vote
					%> Votes
				</h6>
				<h6>
					<%= contestants.female[2].name[0] %> : <%= contestants.female[2].Vote
					%> Votes
				</h6>
			</div>
		</div>

		<div class="image-container mb-5">
			<h2 class="text-center"><%= contestants.male[0].name[0] %></h2>
			<img
				src="../assets/<%= contestants.male[0].name[1] %>.jpeg"
				class="img-fluid"
			/>
			<h3 class="text-center mb-3">
				Total Votes : <%= contestants.male[0].Vote %>
			</h3>
			<div class="d-flex flex-row justify-content-between">
				<h6>
					<%= contestants.male[1].name[0] %> : <%= contestants.male[1].Vote %>
					Votes
				</h6>
				<h6>
					<%= contestants.male[2].name[0] %> : <%= contestants.male[2].Vote %>
					Votes
				</h6>
			</div>
		</div>

		<div class="image-container">
			<h2 class="text-center"><%= contestants.group[0].name[0] %></h2>
			<img
				src="../assets/<%= contestants.group[0].name[1] %>.jpeg"
				class="img-fluid"
			/>
			<h3 class="text-center mb-3">
				Total Votes : <%= contestants.group[0].Vote %>
			</h3>
			<div class="d-flex flex-row justify-content-between">
				<h6>
					<%= contestants.group[1].name[0] %> : <%= contestants.group[1].Vote %>
					Votes
				</h6>
				<h6>
					<%= contestants.group[2].name[0] %> : <%= contestants.group[2].Vote %>
					Votes
				</h6>
			</div>
		</div>
	</div>

	<hr />
	<div class="mb-5">
		<h1 class="mb-3 text-center">Gallery</h1>
		<div id="galleryCarousel" class="carousel slide" data-ride="carousel">
			<ol class="carousel-indicators">
				<li
					data-target="#galleryCarousel"
					data-slide-to="0"
					class="active"
				></li>
				<li data-target="#galleryCarousel" data-slide-to="1"></li>
				<li data-target="#galleryCarousel" data-slide-to="2"></li>
				<li data-target="#galleryCarousel" data-slide-to="3"></li>
				<li data-target="#galleryCarousel" data-slide-to="4"></li>
				<li data-target="#galleryCarousel" data-slide-to="5"></li>
				<li data-target="#galleryCarousel" data-slide-to="6"></li>
				<li data-target="#galleryCarousel" data-slide-to="7"></li>
				<li data-target="#galleryCarousel" data-slide-to="8"></li>
			</ol>
			<div class="carousel-inner">
				<div class="carousel-item active">
					<img src="../assets/female1.jpeg" class="img-fluid" />
				</div>
				<div class="carousel-item">
					<img src="../assets/female2.jpeg" class="img-fluid" />
				</div>
				<div class="carousel-item">
					<img src="../assets/female3.jpeg" class="img-fluid" />
				</div>
				<div class="carousel-item">
					<img src="../assets/male1.jpeg" class="img-fluid" />
				</div>
				<div class="carousel-item">
					<img src="../assets/male2.jpeg" class="img-fluid" />
				</div>
				<div class="carousel-item">
					<img src="../assets/male3.jpeg" class="img-fluid" />
				</div>
				<div class="carousel-item">
					<img src="../assets/group1.jpeg" class="img-fluid" />
				</div>
				<div class="carousel-item">
					<img src="../assets/group2.jpeg" class="img-fluid" />
				</div>
				<div class="carousel-item">
					<img src="../assets/group3.jpeg" class="img-fluid" />
				</div>
			</div>
			<a
				class="carousel-control-prev"
				href="#galleryCarousel"
				role="button"
				data-slide="prev"
			>
				<span class="carousel-control-prev-icon" aria-hidden="true"></span>
				<span class="sr-only">Previous</span>
			</a>
			<a
				class="carousel-control-next"
				href="#galleryCarousel"
				role="button"
				data-slide="next"
			>
				<span class="carousel-control-next-icon" aria-hidden="true"></span>
				<span class="sr-only">Next</span>
			</a>
		</div>
	</div>
</div>

<%- include('../templates/footer'); -%>
